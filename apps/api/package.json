        "nodemon": "^2.0.15",
        "prisma": "^3.9.2",
        "scripts": "*",
        "tsconfig": "*",
        "typescript": "^4.5.3"
    },
    "dependencies": {
        "@prisma/client": "^3.9.2",
        "@types/bcrypt": "^5.0.0",
        "apollo-server": "^3.6.1",
        "bcrypt": "^5.0.1",
        "dotenv": "^10.0.0",
        "graphql": "^16.2.0",
        "jsonwebtoken": "^8.5.1"
    }
